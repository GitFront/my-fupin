/*
	由于节点过多，树标题由接口合成HTML
	标题name格式一致，但会根据页面数据内容会不一样，具体看低保
	HTML格式：
	<span class="name">$1</span><span class="value">$2</span>
	其中：
	$1：区域名
	$2：数据值

	支持整树加载和异步加载。
	异步加载前需要先调用此接口获得指定路径所需展开的所有节点的数据
	之后点击有需要异步加载的节点通过另一个接口查询子树
*/
{
	"code": 0,
	"msg": "获取信息成功",
	"data": {
		//提示内容，具体内容视乎低保
		"tip": "数字为统计年内人均可支配收入，单位：元",
		//根据传入的区域级别与区域ID得到树中的唯一ID
		"match_node_id": 4,
		/*
			区域树数据
			节点ID为全树唯一，用于查找的标记
			区域级别同其他接口区域级别取值
		*/
		"tree": [
			{
				//节点ID
				"id": 1,
				//标题
				"name": "父节点1",
				//区域级别，用于其他接口查找数据
				"area_level": "province",
				//区域ID，用于其他接口查找数据
				"area_id": "440",
				/*
					是否父节点且需要异步加载子节点的节点
					有子节点且是需要从异步加载节点才为true
				*/
				"isParent": true,
				//子节点列表，如无没有此属性
				"children": [
					{
						//节点ID
						"id": 2,
						//标题
						"name": "子节点1",
						//父节点ID，根级节点没有
						"pId": 1,
						//区域级别，用于其他接口查找数据
						"area_level": "city",
						//区域ID，用于其他接口查找数据
						"area_id": "440",
						//子节点列表，如无没有此属性
						"children": [
							{
								//节点ID
								"id": 4,
								//父节点ID，根级节点没有
								"pId": 2,
								//区域级别，用于其他接口查找数据
								"area_level": "town",
								//区域ID，用于其他接口查找数据
								"area_id": "440",
								//标题
								"name": "孙节点1"
							}
						]
					},
					{
						//节点ID
						"id": 3,
						//父节点ID，根级节点没有
						"pId": 1,
						//标题
						"name": "子节点2",
						//区域级别，用于其他接口查找数据
						"area_level": "city",
						//区域ID，用于其他接口查找数据
						"area_id": "440"
					}
				]
			}
		]
	}
}

