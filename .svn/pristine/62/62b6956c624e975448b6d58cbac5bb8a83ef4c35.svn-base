@charset "UTF-8";

@font-face{
  font-family: dongQingHeiTi;
  src: url('../../font/Hiragino Sans GB W3.otf');
}

@font-face{
  font-family: miNiJian;
  src: url('../../font/impact.otf'),
          url('../../font/miNiJian.ttf');
}

/* 引入scss文件 */
/* base基础文件 */
@import "base/_reset";//重置浏览器默认属性
@import "base/_var";//sass变量
@import "base/_function";//函数
@import "base/_mixin_strategy";//混合
@import "base/_base_strategy";//基本样式
@import "base/_animation";//动画
/* end base */

/* layout 重复布局 */
@import "layout/_header_strategy";//头部导航
@import "layout/_center_strategy";//页面中部布局-现状基本是贫困分析的中部
@import "layout/_footer";
/* end layout */

/* page 页面 */
@import "page/_poor_county_detail";//贫困村档案 和 贫困户档案
@import "page/_strategy";//战略地图
@import "page/_data";//数据监控
@import "page/_search";
@import "page/_search_result";
@import "page/_edu";
@import "page/_edu_school";
/* end page */
